<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="es.dam.view.TableViewController">
   <children>
      <Button fx:id="btnAgregarPersona" layoutX="47.0" layoutY="413.0" mnemonicParsing="false" onAction="#anadirPersona" prefHeight="26.0" prefWidth="162.0" text="Agregar Persona" />
      <Button fx:id="btnEdit" layoutX="245.0" layoutY="413.0" mnemonicParsing="false" onAction="#editarPersona" prefHeight="26.0" prefWidth="162.0" text="Editar Persona" />
      <Button fx:id="btnEliminar" layoutX="454.0" layoutY="413.0" mnemonicParsing="false" onAction="#deletePersona" prefHeight="26.0" prefWidth="162.0" text="Eliminar Persona" />
      <TableView fx:id="tblPersonas" layoutX="49.0" layoutY="40.0" onMouseClicked="#seleccionar" prefHeight="326.0" prefWidth="551.0">
        <columns>
          <TableColumn fx:id="columNombre" prefWidth="159.0" text="nombre" />
          <TableColumn fx:id="columApellido" prefWidth="282.0" text="apellido" />
            <TableColumn fx:id="columEdad" prefWidth="109.0" text="edad" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
