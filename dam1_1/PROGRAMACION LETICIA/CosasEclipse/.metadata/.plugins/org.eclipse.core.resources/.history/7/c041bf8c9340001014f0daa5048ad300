	public static int[] findDigit(int number, int digit) {
		int[] respuesta = new int[1];
		
		String numbers = Integer.toString(number);
		
		System.out.println(numbers);
		
		return respuesta;
	}
	
	
	public static void main(String[] args) {
		//p1 
