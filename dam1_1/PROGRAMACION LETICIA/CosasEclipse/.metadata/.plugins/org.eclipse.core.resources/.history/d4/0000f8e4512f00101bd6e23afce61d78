package puto;

public class camelCaseConverter {
	//convertir una frase a camel case
	// frase antes: tejero maricon
	// frase despues: tejeroMaricon
	public static void main(String[] args) {
		String tuprima= "a";
		
		char b = tuprima.charAt(0);
	
	
		
		char c = (char) (b - 32) ;
		
		System.out.println(c);
	}
	
}
